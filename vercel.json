{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/avatar.js",
      "dest": "/avatar.js",
      "headers": { "Cache-Control": "no-cache" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
